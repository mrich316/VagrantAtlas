<?xml version="1.0" encoding="utf-8" ?>
<parameters>

  <parameter
    name="appSettings/atlas:id"
    defaultValue="vagrant"
    description="Vagrant Box Administrator">
    <parameterEntry
      kind="XmlFile"
      scope="\\web.config$"
      match="/configuration/appSettings/add[@key='atlas:id']/@value" />
  </parameter>

  <parameter
    name="appSettings/atlas:secret"
    defaultValue="vagrant"
    description="Vagrant Box Administrator Password">
    <parameterEntry
      kind="XmlFile"
      scope="\\web.config$"
      match="/configuration/appSettings/add[@key='atlas:secret']/@value" />
  </parameter>

</parameters>